<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE TaskDefinition PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<TaskDefinition executor="sailpoint.plugin.helloworld.task.HelloWorldTaskExecutor" name="Hello World Task" progressInterval="5000" progressMode="String" type="Generic">
    <Description>
        Example Hello World plugin task
    </Description>
    <Attributes>
        <Map>
            <entry key='pluginName' value='HelloWorld'/>
        </Map>
    </Attributes>
    <Signature>
        <Returns>
            <Argument name="total" type="int">
                <Prompt>total</Prompt>
            </Argument>
        </Returns>
    </Signature>
</TaskDefinition>
